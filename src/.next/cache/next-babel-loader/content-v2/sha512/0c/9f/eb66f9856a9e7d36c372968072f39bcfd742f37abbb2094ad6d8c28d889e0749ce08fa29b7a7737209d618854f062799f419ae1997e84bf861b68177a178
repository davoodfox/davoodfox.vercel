 send] = useMachine(tipMachine);\n\n  return (\n    <>\n      <h1>{state.value}</h1>\n      <Button\n        type=\"primary\"\n        onClick={() => {\n          send(\"OPEN\");\n        }}\n      >\n        Open Modal\n      </Button>\n      <Modal\n        title=\"Options\"\n        visible={state.matches(\"options\")}\n        okText=\"Continue\"\n        onOk={() => {\n          console.log(tip);\n          if (tip) {\n            send(\"CONTINUE\");\n          } else {\n            return;\n          }\n        }}\n        onCancel={() => {\n          send(\"CLOSE\");\n        }}\n      >\n        <h3>Product is ${value}</h3>\n        <h1>Tip for {name}</h1>\n        <Button\n          type={percent == 5 && \"primary\"}\n          onClick={() => {\n            setTip((value * 5) / 100);\n            setPercent(5);\n          }}\n        >\n          5%\n        </Button>\n        <Button\n          type={percent == 10 && \"primary\"}\n          onClick={() => {\n            setTip((value * 10) / 100);\n            setPercent(10);\n          }}\n        >\n          10%\n        </Button>\n        <Button\n          type={percent == 15 && \"primary\"}\n          onClick={() => {\n            setTip((value * 15) / 100);\n            setPercent(15);\n          }}\n        >\n          15%\n        </Button>\n        <Button\n          type={percent == 20 && \"primary\"}\n          onClick={() => {\n            setTip((value * 20) / 100);\n            setPercent(20);\n          }}\n        >\n          20%\n        </Button>\n        <Input\n          addonBefore=\"$\"\n          type=\"number\"\n          placeholder=\"custom value\"\n          onFocus={(e) => {\n            setTip(e.target.value);\n            setPercent(null);\n          }}\n        />\n      </Modal>\n      <Modal\n        title=\"Details\"\n        visible={state.matches(\"details\")}\n        okText=\"Confirm\"\n        onOk={() => {\n          send(\"CONTINUE\");\n        }}\n        cancelText=\"Back\"\n        onCancel={() => {\n          send(\"BACK\");\n        }}\n      >\n        <h1>Tip for {name}</h1>\n        <h3>${tip}</h3>\n      </Modal>\n    </>\n  );\n}\n\nexport default TipModal;\n"]},"metadata":{},"sourceType":"module"}